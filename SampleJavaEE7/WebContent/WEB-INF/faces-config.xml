<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

	<application>
		<action-listener>test.fw.BaseActionListener</action-listener>
	</application>

	<lifecycle>
		<phase-listener>test.fw.BaseRestoreViewPhaseListener</phase-listener>
		<phase-listener>test.fw.BaseApplyRequestValuesPhaseListener</phase-listener>
		<phase-listener>test.fw.BaseProcessValidationsPhaseListener</phase-listener>
		<phase-listener>test.fw.BaseUpdateModelValuesPhaseListener</phase-listener>
		<phase-listener>test.fw.BaseInvokeApplicationPhaseListener</phase-listener>
		<phase-listener>test.fw.BaseRenderResponsePhaseListener</phase-listener>
	</lifecycle>

</faces-config>
